                          <h2 class="mb-2"><%= count %> </h2>
                          <span>Total Orders</span>
                        </div>
                        <div id="orderStatisticsChart"></div>
                      </div>
                      <ul class="p-0 m-0">
                        <li class="d-flex mb-4 pb-1">
                          <div class="avatar flex-shrink-0 me-3">
                            <span class="avatar-initial rounded bg-label-primary"><iconify-icon icon="carbon:delivery-truck"></iconify-icon></span>
                          </div>
                          <div class="d-flex w-100 flex-wrap align-items-center justify-content-between gap-2">
                            <div class="me-2">
                              <h6 class="mb-0">Cash On Delivery</h6>
                              <small class="text-muted">Shipped,delivered,placed</small>
                            </div>
                            <div class="user-progress">
                              <small class="fw-semibold"><%= codCount %> </small>
                            </div>
                          </div>
                        </li>
                        <li class="d-flex mb-4 pb-1">
                          <div class="avatar flex-shrink-0 me-3">
                            <span class="avatar-initial rounded bg-label-secondary"><i
                                class="bx bx-mobile-alt"></i></span>
                          </div>
                          <div class="d-flex w-100 flex-wrap align-items-center justify-content-between gap-2">
                            <div class="me-2">
                              <h6 class="mb-0">Online Payment</h6>
                              <small class="text-muted">Shipped,delivered,placed</small>
                            </div>
                            <div class="user-progress">
                              <small class="fw-semibold"><%= onlineCount %> </small>
                            </div>
                          </div>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
